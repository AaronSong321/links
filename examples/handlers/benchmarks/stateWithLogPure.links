# Microbenchmark stateful counting with logging (pure)
fun countPureLog(i, is) {
    if (i == 0) {
      (i, is)
    } else {
      countPureLog(i-1, (i :: is))
    }
}

fun runPureCount(n) {
    countPure(n)
}

fun runPureCountLog(n) {
    first(countPureLog(n, []))
}

fun b(n) {
    var x = runPureCountLog(n);
    ()
}

b(10^7)
