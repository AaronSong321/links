fun maybeResult(x) {
    handle(x) {
    	      case Fail(_,_)  -> Nothing
	      case Return(x)  -> Just(x)
    }
}

fun maybeResult2(x) {
    handle(x) {
    	      case Fail(p,_) -> Just(p)
    	      case Return(x) -> Nothing
    }
}

