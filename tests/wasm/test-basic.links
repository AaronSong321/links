fun addAll(n) {
	fun alt(n, acc) {
		if (n == 0) acc
		else alt(n-1, acc*n)
	}
	alt(n, 1)
}
fun addClosure(x) {
	var y = x + x;
	fun addAnInt(t) {
		t+y
	}
	addAnInt
}
fun addRecord(b) {
	var a = (m=3,n=15);
	a.m + b
}
fun addVariant(c) {
	switch (c) {
		case A(a,b,m) -> 10
		#case -A as x -> 1
		case B() -> 8
	}
}
fun test() {
	#var _ = addAll(1000000);
	#var add16 = addClosure(8);
	#var _ = add16(7);
	#var f2 = addVariant(A(8,"mnd",5.5);
	#var f1 = addRecord(8);
	()
}
test()